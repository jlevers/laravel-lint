{
    "name": "jlevers/laravel-lint",
    "description": "Opinionated Linting for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "sinkcup",
            "email": "sinkcup@gmail.com"
        },
        {
            "name": "Jesse Evers",
            "email": "jesse@jesseevers.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LaravelLint\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app/",
            "LaravelLint\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLint\\LintServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "require": {
        "ext-json": "*",
        "illuminate/support": ">=v6.20.36",
        "phpmd/phpmd": "^2.10",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "require-dev": {
        "orchestra/testbench": ">=v4.8.0"
    }
}
